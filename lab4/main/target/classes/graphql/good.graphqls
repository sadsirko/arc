
type Good {
    id: ID!
    name: String ,
    price: Float,
    amount:Int,
    brand:String,
    manufacturer:String,
    discount:Int

}

type Query {
good(id: ID):Good
}

type Mutation {
createGood(name: String ,
    price: Float,
    amount:Int,
    brand:String,
    manufacturer:String,
    discount:Int):Good
}