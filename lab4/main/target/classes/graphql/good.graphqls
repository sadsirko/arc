
type Good {
    id: ID!
    name: String ,
    price: Double,
    amount:Integer,
    brand:String,
    manufacturer:String,
    discount:Integer

}

type Query {
goods(count: Int):[Good]
vehicle(id: ID):Good
}

type Mutation {
createGood(name: String ,
    price: Double,
    amount:Integer,
    brand:String,
    manufacturer:String,
    discount:Integer):Good
}